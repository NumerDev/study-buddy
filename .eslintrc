{
  "extends": ["react-app", "react-app/jest", "plugin:prettier/recommended", "prettier"],

  "plugins": ["prettier"],

  "rules": {
    "jsx-a11y/img-redundant-alt": "off",
    "prettier/prettier": [
      "warn",
      {
        "singleQuote": true,
        "endOfLine": "auto",
        "printWidth": 150
      }
    ]
    // "import/first": 0
  },

  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },

  "parser": "@babel/eslint-parser"
}
